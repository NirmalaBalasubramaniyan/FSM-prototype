<div class="setup-left">
	
	<div>
	<ul>
			<li class="fl">
				<a href="index.html" style="text-decoration: none;color: #222;"><p class="lft-setup-tit setup-tit">Setup</p></a>
			</li>
			<li class="fl"> 
				<input type="text" placeholder="Search" class="lft-setup-search mt20">
			</li>
			
		</ul>
</div>
	<div class="setup-lft-menu">
	{{#each @value as |stgGrp|}}
		<ul class="stg-group">
			<b><li class="stg-title" {{on "click" this.toggleSubSettings}}>{{stgGrp.title}}</li></b>
			<div class="side-stg-submenus" style="display: none">
			{{#each stgGrp.submenus as |stgItem|}}
				<li class="stg-item"><LinkTo @route="setting" @model={{stgItem.url}}>{{stgItem.title}}</LinkTo></li>
			{{/each}}
			</div>
		</ul>
	{{/each}}
</div>
</div>